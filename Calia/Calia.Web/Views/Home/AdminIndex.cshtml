@using Calia.Web.Utility
@model RaporVM
@{
    ViewData["Title"] = "Menu Page";
    Layout = "~/Views/Shared/_LayoutCallia.cshtml";
}


<div class="row gy-4">
    <div class="col-xxl-12">
        <div class="card radius-8 border-0">
            <div class="row">
                <div class="col-xxl-6 pe-xxl-0">
                    <div class="card-body p-24">
                        <div class="d-flex align-items-center flex-wrap gap-2 justify-content-between">
                            <h6 class="mb-2 fw-bold text-lg">Kredi-Nakit Tablosu</h6>
                            <div>
                                <select id="timePeriodSelect" class="form-select form-select-sm w-auto bg-base border text-secondary-light">
                                    <option value="weekly">Haftalık</option>
                                    <option value="monthly">Aylık</option>
                                </select>
                            </div>
                        </div>
                        <ul class="d-flex flex-wrap align-items-center mt-3 gap-3">
                            <li class="d-flex align-items-center gap-2">
                                <span class="w-12-px h-12-px radius-2 bg-primary-600"></span>
                                <span class="text-secondary-light text-sm fw-semibold">
                                    Kredi Kartı:
                                    <span class="text-primary-light fw-bold">@string.Format(new System.Globalization.CultureInfo("tr-TR"), "{0:C}", Model.YillikKrediKarti)</span>
                                </span>
                            </li>
                            <li class="d-flex align-items-center gap-2">
                                <span class="w-12-px h-12-px radius-2 bg-yellow"></span>
                                <span class="text-secondary-light text-sm fw-semibold">
                                    Nakit:
                                    <span class="text-primary-light fw-bold">@string.Format(new System.Globalization.CultureInfo("tr-TR"), "{0:C}", Model.YillikNakit)</span>
                                </span>
                            </li>
                        </ul>
                        <div class="mt-40">
                            <div id="paymentStatusChart" class="margin-16-minus"></div>
                        </div>
                    </div>
                </div>
                <div class="col-xxl-6">
                    <div class="row h-100 g-0">
                        <div class="col-6 p-0 m-0">
                            <div class="card-body p-24 h-100 d-flex flex-column justify-content-center border border-top-0">
                                <div class="d-flex flex-wrap align-items-center justify-content-between gap-1 mb-8">
                                    <div>
                                        <span class="mb-12 w-44-px h-44-px text-primary-600 bg-primary-light border border-primary-light-white flex-shrink-0 d-flex justify-content-center align-items-center radius-8 h6 mb-12">
                                            <iconify-icon icon="fa-solid:box-open" class="icon"></iconify-icon>
                                        </span>
                                        <span class="mb-1 fw-medium text-secondary-light text-md">Toplam Ürün Sayısı</span>
                                        <h6 class="fw-semibold text-primary-light mb-1">@Model.Veriler.TotalProductNumber</h6>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-6 p-0 m-0">
                            <div class="card-body p-24 h-100 d-flex flex-column justify-content-center border border-top-0 border-start-0 border-end-0">
                                <div class="d-flex flex-wrap align-items-center justify-content-between gap-1 mb-8">
                                    <div>
                                        <span class="mb-12 w-44-px h-44-px text-yellow bg-yellow-light border border-yellow-light-white flex-shrink-0 d-flex justify-content-center align-items-center radius-8 h6 mb-12">
                                            <iconify-icon icon="flowbite:users-group-solid" class="icon"></iconify-icon>
                                        </span>
                                        <span class="mb-1 fw-medium text-secondary-light text-md">Toplam Kapanan Masa Sayısı</span>
                                        <h6 class="fw-semibold text-primary-light mb-1">@Model.Veriler.ToplamKapananMasaSayisi</h6>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-6 p-0 m-0">
                            <div class="card-body p-24 h-100 d-flex flex-column justify-content-center border border-top-0 border-bottom-0">
                                <div class="d-flex flex-wrap align-items-center justify-content-between gap-1 mb-8">
                                    <div>
                                        <span class="mb-12 w-44-px h-44-px text-lilac bg-lilac-light border border-lilac-light-white flex-shrink-0 d-flex justify-content-center align-items-center radius-8 h6 mb-12">
                                            <iconify-icon icon="majesticons:shopping-cart" class="icon"></iconify-icon>
                                        </span>
                                        <span class="mb-1 fw-medium text-secondary-light text-md">Toplam Sipariş Sayısı</span>
                                        <h6 class="fw-semibold text-primary-light mb-1">@Model.Veriler.ToplamSiparisSayisi</h6>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-6 p-0 m-0">
                            <div class="card-body p-24 h-100 d-flex flex-column justify-content-center border border-top-0 border-start-0 border-end-0 border-bottom-0">
                                <div class="d-flex flex-wrap align-items-center justify-content-between gap-1 mb-8">
                                    <div>
                                        <span class="mb-12 w-44-px h-44-px text-pink bg-pink-light border border-pink-light-white flex-shrink-0 d-flex justify-content-center align-items-center radius-8 h6 mb-12">
                                            <iconify-icon icon="ri:discount-percent-fill" class="icon"></iconify-icon>
                                        </span>
                                        <span class="mb-1 fw-medium text-secondary-light text-md">Toplam Kazanç</span>
                                        <h6 class="fw-semibold text-primary-light mb-1">@Model.Veriler.ToplamKazanç</h6>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="col-xxl-12 col-lg-6">
        <div class="card h-100">
            <div class="card-body p-24">
                <div class="d-flex align-items-center flex-wrap gap-2 justify-content-between mb-20">
                    <h6 class="mb-2 fw-bold text-lg mb-0">Masaların Günlük Raporu</h6>
                    <a asp-controller="Home" asp-action="MasaGunlukRapor" class="text-primary-600 hover-text-primary d-flex align-items-center gap-1">
                        Tümünü Gör
                        <iconify-icon icon="solar:alt-arrow-right-linear" class="icon"></iconify-icon>
                    </a>
                </div>
                <div class="table-responsive scroll-sm">
                    <table class="table bordered-table mb-0">
                        <thead>
                            <tr>
                                <th scope="col">Id</th>
                                <th scope="col">Masa</th>
                                <th scope="col">Açılış</th>
                                <th scope="col">Kapanış</th>
                                <th scope="col">Toplam Ücret</th>
                                <th scope="col" class="text-center">Durum</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var masa in Model.MasalarınGünlükRaporu)
                            {
                                <tr onclick="window.location='@Url.Action("TableDetail", "Home", new { id = masa.Id })'" style="cursor:pointer;">
                                    <td>@masa.Id</td>
                                    <td>
                                        <div class="d-flex align-items-center">
                                            <span class="text-lg text-secondary-light fw-semibold flex-grow-1">@masa.tableNo.MasaNo</span>
                                        </div>
                                    </td>
                                    <td>@masa.OpenTime?.ToString("HH:mm")</td>
                                    <td>
                                        @if (masa.CloseTime != null)
                                        {
                                            @masa.CloseTime?.ToString("HH:mm")
                                        }
                                        else
                                        {
                                            <span class="text-warning fw-bold">Kapanmadı</span>
                                        }
                                    </td>
                                    <td>@string.Format(new System.Globalization.CultureInfo("tr-TR"), "{0:C}", masa.AlinanFiyat)</td>
                                    <td class="text-center">
                                        @if (masa.PaymentStatus == SD.PaymentStatusApproved)
                                        {
                                            <span class="bg-success-focus text-success-main px-24 py-4 rounded-pill fw-medium text-sm">
                                                @masa.PaymentStatus
                                            </span>
                                        }
                                        else
                                        {
                                            <span class="bg-danger-focus text-danger-main px-24 py-4 rounded-pill fw-medium text-sm">
                                                @masa.PaymentStatus
                                            </span>
                                        }
                                    </td>
                                </tr>
                            }


                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
    <div class="col-xxl-6 col-lg-6">
        <div class="card h-100">
            <div class="card-body p-24">
                <div class="d-flex align-items-center flex-wrap gap-2 justify-content-between mb-20">
                    <h6 class="mb-2 fw-bold text-lg mb-0">İptaller</h6>
                    <a asp-controller="Home" asp-action="Iptaller" class="text-primary-600 hover-text-primary d-flex align-items-center gap-1">
                        Tümünü Gör
                        <iconify-icon icon="solar:alt-arrow-right-linear" class="icon"></iconify-icon>
                    </a>
                </div>
                <div class="table-responsive scroll-sm">
                    <table class="table bordered-table mb-0">
                        <thead>
                            <tr>
                                <th scope="col">Ürünler</th>
                                <th scope="col">Masa No</th>
                                <th scope="col">İptal Saati</th>
                                <th scope="col">Miktar</th>
                                <th scope="col">Fiyat</th>
                                <th scope="col" class="text-center">Durum</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var iptal in Model.İptaller)
                            {
                                <tr>
                                    <td>
                                        <div class="d-flex align-items-center">
                                            <span class="text-lg text-secondary-light fw-semibold flex-grow-1">@iptal.ProductName</span>
                                        </div>
                                    </td>
                                    <td>@iptal.MasaNo</td>
                                    <td>@iptal.CancelTime?.ToString("HH:mm")</td>
                                    <td>@iptal.Count</td>
                                    <td>@string.Format(new System.Globalization.CultureInfo("tr-TR"), "{0:C}", iptal.Count * iptal.Price)</td>
                                    <td class="text-center"> <span class="bg-danger-focus text-danger-main px-24 py-4 rounded-pill fw-medium text-sm">İptal Edildi</span> </td>
                                </tr>
                            }

                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
    <div class="col-xxl-6 col-lg-6">
        <div class="card h-100">
            <div class="card-body p-24">
                <div class="d-flex align-items-center flex-wrap gap-2 justify-content-between mb-20">
                    <h6 class="mb-2 fw-bold text-lg mb-0">Masaların Toplam Kazancı</h6>
                    <a asp-controller="Home" asp-action="MasaToplamKazanc" class="text-primary-600 hover-text-primary d-flex align-items-center gap-1">
                        Tümünü Gör
                        <iconify-icon icon="solar:alt-arrow-right-linear" class="icon"></iconify-icon>
                    </a>
                </div>
                <div class="table-responsive">
                    <table class="table vertical-striped-table mb-0">
                        <thead>
                            <tr>
                                <th scope="col">Masa Numarası</th>
                                <th scope="col">Toplam Kazanç</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model.MasalarınToplamKazancı)
                            {
                                <tr>
                                    <td>
                                        <h6 class="text-md mb-0 fw-normal">@item.Key</h6>
                                    </td>
                                    <td>@string.Format(new System.Globalization.CultureInfo("tr-TR"), "{0:C}", item.Value)</td>
                                </tr>
                            }


                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
    <div class="col-xxl-4">
        <div class="card h-100">

            <div class="card-body">
                <div class="d-flex align-items-center flex-wrap gap-2 justify-content-between">
                    <h6 class="mb-2 fw-bold text-lg">Günlük Gelir-Gider Raporu</h6>

                </div>


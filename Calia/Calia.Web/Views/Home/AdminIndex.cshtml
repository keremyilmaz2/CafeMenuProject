    </div>
    <div class="col-xxl-4">
        <div class="card h-100 radius-8 border-0">
            <div class="card-body p-24">
                <div class="d-flex align-items-center flex-wrap gap-2 justify-content-between">
                    <h6 class="mb-2 fw-bold text-lg">Nakit - Kredi - İskonto - İkram Oranı</h6>
                </div>

                <div class="position-relative">
                    <div id="statisticsDonutChart" class="mt-36 flex-grow-1 apexcharts-tooltip-z-none title-style circle-none"></div>
                </div>

                <ul class="d-flex flex-wrap align-items-center justify-content-between mt-3 gap-3">
                    <li class="d-flex align-items-center gap-2">
                        <span class="w-12-px h-12-px radius-2 bg-primary-600"></span>
                        <span class="text-secondary-light text-sm fw-normal">
                            Kredi Kartı:
                            <span class="text-primary-light fw-bold">@Model.GünlükGelirGiderRaporu["KrediKarti"]</span>
                        </span>
                    </li>
                    <li class="d-flex align-items-center gap-2">
                        <span class="w-12-px h-12-px radius-2 bg-yellow"></span>
                        <span class="text-secondary-light text-sm fw-normal">
                            Nakit:
                            <span class="text-primary-light fw-bold">@Model.GünlükGelirGiderRaporu["Nakit"]</span>
                        </span>
                    </li>
                    <li class="d-flex align-items-center gap-2">
                        <span class="w-12-px h-12-px radius-2 bg-green" style="background-color:#4CAF50"></span>
                        <span class="text-secondary-light text-sm fw-normal">
                            İskonto:
                            <span class="text-primary-light fw-bold">@Model.GünlükGelirGiderRaporu["Iskonto"]</span>
                        </span>
                    </li>
                    <li class="d-flex align-items-center gap-2">
                        <span class="w-12-px h-12-px radius-2 bg-red"></span>
                        <span class="text-secondary-light text-sm fw-normal">
                            İkram:
                            <span class="text-primary-light fw-bold">@Model.GünlükGelirGiderRaporu["Ikram"]</span>
                        </span>
                    </li>
                </ul>
            </div>
        </div>
    </div>

    <div class="col-xxl-6">
        <div class="card h-100">
            <div class="card-body p-24">
                <div class="d-flex align-items-center flex-wrap gap-2 justify-content-between mb-20">
                    <h6 class="mb-2 fw-bold text-lg mb-0">Satılan Ürünler</h6>
                    <a asp-controller="Home" asp-action="SatilanUrunler" class="text-primary-600 hover-text-primary d-flex align-items-center gap-1">
                        Tümünü Gör
                        <iconify-icon icon="solar:alt-arrow-right-linear" class="icon"></iconify-icon>
                    </a>
                </div>
                <div class="table-responsive">
                    <table class="table vertical-striped-table mb-0">
                        <thead>
                            <tr>
                                <th scope="col">Ürünler</th>
                                <th scope="col" class="text-center">Toplam Kazanç</th>
                                <th scope="col" class="text-center">Sipariş Sayısı</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var satilanUrunler in Model.UrunBasinaSatilma)
                            {
                                <tr>
                                    <td>
                                        <h6 class="text-md mb-0 fw-normal">@satilanUrunler.ProductName</h6>
                                        <span class="text-sm text-secondary-light fw-normal">@satilanUrunler.CategoryName</span>
                                    </td>
                                    <td class="text-center">@string.Format(new System.Globalization.CultureInfo("tr-TR"), "{0:C}", satilanUrunler.Price)</td>
                                    <td class="text-center">
                                        <span class="bg-success-focus text-success-main px-32 py-4 rounded-pill fw-medium text-sm">@satilanUrunler.SatilmaAdedi</span>
                                    </td>
                                </tr>

                            }

                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
    <div class="col-xxl-6">
        <div class="card h-100">
            <div class="card-body p-24">
                <div class="d-flex align-items-center flex-wrap gap-2 justify-content-between mb-20">
                    <h6 class="mb-2 fw-bold text-lg mb-0">Kalan Tatlılar</h6>
                    <a asp-controller="Home" asp-action="KalanTatlilar" class="text-primary-600 hover-text-primary d-flex align-items-center gap-1">
                        Tümünü Gör
                        <iconify-icon icon="solar:alt-arrow-right-linear" class="icon"></iconify-icon>
                    </a>
                </div>
                <div class="table-responsive scroll-sm">
                    <table class="table bordered-table mb-0">
                        <thead>
                            <tr>
                                <th scope="col">Tatlılar</th>
                                <th scope="col">Fiyat</th>
                                <th scope="col">
                                    <div class="max-w-112 mx-auto">
                                        <span>Stok</span>
                                    </div>
                                </th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var kalanTatlilar in Model.KalanTatlılar)
                            {
                                // Stok durumu
                                int stockLevel = kalanTatlilar.AvailableProducts; // Kalan tatlının stok miktarı
                                string stockStatus; // Stok durumu için mesaj
                                string progressBarColor; // İlerleme çubuğunun rengi
                                double progressPercentage; // İlerleme çubuğu yüzdesi

                                // Stok durumuna göre ayarları belirle
                                if (stockLevel > 15)
                                {
                                    stockStatus = "Yüksek Stok";
                                    progressBarColor = "bg-success"; // Yeşil renk
                                    progressPercentage = 100; // %100
                                }
                                else if (stockLevel > 5)
                                {
                                    stockStatus = "Orta Stok";
                                    progressBarColor = "bg-warning"; // Sarı renk
                                    progressPercentage = (double)stockLevel / 20 * 100; // Stok seviyesine göre yüzde
                                }
                                else
                                {
                                    stockStatus = "Düşük Stok";
                                    progressBarColor = "bg-danger"; // Kırmızı renk
                                    progressPercentage = (double)stockLevel / 20 * 100; // Stok seviyesine göre yüzde
                                }

                                <tr>
                                    <td>@kalanTatlilar.Name</td>
                                    <td>@string.Format(new System.Globalization.CultureInfo("tr-TR"), "{0:C}", kalanTatlilar.Price)</td>
                                    <td>
                                        <div class="max-w-112 mx-auto">
                                            <div class="w-100">
                                                <div class="progress progress-sm rounded-pill" role="progressbar" aria-label="Stock Status" aria-valuenow="@progressPercentage" aria-valuemin="0" aria-valuemax="100">
                                                    <div class="progress-bar @progressBarColor rounded-pill" style="width: @progressPercentage%;"></div>
                                                </div>
                                            </div>
                                            <span class="mt-12 text-secondary-light text-sm fw-medium">@stockStatus - @stockLevel adet kaldı</span>
                                        </div>
                                    </td>
                                </tr>
                            }



                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
    <div class="col-xxl-6 col-lg-6">
        <div class="card h-100">
            <div class="card-body p-24">
                <div class="d-flex align-items-center flex-wrap gap-2 justify-content-between mb-20">
                    <h6 class="mb-2 fw-bold text-lg mb-0">Garsonlarin Siparis Sayisi</h6>
                    
                </div>
                <div class="table-responsive">
                    <table class="table vertical-striped-table mb-0">
                        <thead>
                            <tr>
                                <th scope="col">Garson</th>
                                <th scope="col">Satis Sayisi</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model.GünlükGarsonRaporu)
                            {
                                <tr>
                                    <td>
                                        <h6 class="text-md mb-0 fw-normal">@item.Key</h6>
                                    </td>
                                    <td>@item.Value</td>
                                </tr>
                            }


                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>

    <div class="col-xxl-6">
        <div class="card h-100">
            <div class="card-body p-24">
                <div class="d-flex align-items-center flex-wrap gap-2 justify-content-between mb-20">
                    <h6 class="mb-2 fw-bold text-lg mb-0">İkram Edilen Ürünler</h6>
                    <a asp-controller="Home" asp-action="IkramEdilenUrunler" class="text-primary-600 hover-text-primary d-flex align-items-center gap-1">
                        Tümünü Gör
                        <iconify-icon icon="solar:alt-arrow-right-linear" class="icon"></iconify-icon>
                    </a>
                </div>
                <div class="table-responsive">
                    <table class="table vertical-striped-table mb-0">
                        <thead>
                            <tr>
                                <th scope="col">Ürünler</th>
                                <th scope="col" class="text-center">İskonto</th>
                                <th scope="col" class="text-center">İkram Sayısı</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var ikramUrunler in Model.IkramVerilenUrunler)
                            {
                                <tr>
                                    <td>
                                        <h6 class="text-md mb-0 fw-normal">@ikramUrunler.ProductName</h6>
                                        <span class="text-sm text-secondary-light fw-normal">@ikramUrunler.CategoryName</span>
                                    </td>
                                    <td class="text-center">@string.Format(new System.Globalization.CultureInfo("tr-TR"), "{0:C}", ikramUrunler.Price)</td>
                                    <td class="text-center">
                                        <span class="bg-success-focus text-success-main px-32 py-4 rounded-pill fw-medium text-sm">@ikramUrunler.IkramAdedi</span>
                                    </td>
                                </tr>

                            }

                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>


@section Scripts {
    <script>
        // C# nesnesini JSON'a dönüştür
        var haftalikNakit = @Html.Raw(Json.Serialize(Model.Veriler.HaftalikNakit));
        var haftalikKrediKarti = @Html.Raw(Json.Serialize(Model.Veriler.HaftalikKrediKarti));
        var aylikNakit = @Html.Raw(Json.Serialize(Model.Veriler.AylikNakit));
        var aylikKrediKarti = @Html.Raw(Json.Serialize(Model.Veriler.AylikKrediKarti));

        var nakit = @Html.Raw(Json.Serialize(Model.GünlükGelirGiderRaporu["Nakit"]));
        var krediKarti = @Html.Raw(Json.Serialize(Model.GünlükGelirGiderRaporu["KrediKarti"]));
        var iskonto = @Html.Raw(Json.Serialize(Model.GünlükGelirGiderRaporu["Iskonto"]));
        var ikram = @Html.Raw(Json.Serialize(Model.GünlükGelirGiderRaporu["Ikram"]));
        // Haftalık ve aylık veri setleri
        var weeklySeries = [{
            name: 'Kredi Karti',
            data: haftalikKrediKarti
        }, {
            name: 'Nakit',
            data: haftalikNakit
        }];

        var monthlySeries = [{
            name: 'Kredi Karti',
            data: aylikKrediKarti
        }, {
            name: 'Nakit',
            data: aylikNakit
        }];
    </script>


    <script src="~/js/homeThreeChart.js"></script>
}